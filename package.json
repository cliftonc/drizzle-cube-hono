{
  "name": "drizzle-cube-hono",
  "version": "1.0.0",
  "description": "Example hono app https://try.drizzle-cube.dev",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "tsx watch src/index.ts",
    "dev:client": "cd client && vite",
    "dev:full": "concurrently \"npm:dev:worker\" \"npm:dev:client\"",
    "build": "npm run build:client",
    "build:client": "cd client && tsc && vite build",
    "start": "node dist/index.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "setup": "npm run docker:up && sleep 5 && npm run db:generate && npm run db:migrate && npm run db:seed",
    "setup:neon": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "typecheck": "tsc --noEmit",
    "ci:all": "npm ci"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@neondatabase/serverless": "^1.0.1",
    "@nivo/heatmap": "^0.99.0",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "dotenv": "^17.2.1",
    "drizzle-cube": "^0.4.7",
    "drizzle-orm": "^0.45.0",
    "hono": "^4.0.0",
    "postgres": "^3.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-grid-layout": "^2.1.1",
    "react-is": "^19.2.3",
    "react-resizable": "^3.0.5",
    "reactflow": "^11.11.4",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-grid-layout": "^1.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.0.0",
    "drizzle-kit": "^0.31.4",
    "postcss": "^8.4.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0",
    "vite": "^7.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0",
    "lightningcss-linux-x64-gnu": "^1.30.0"
  },
  "keywords": [
    "analytics",
    "drizzle",
    "hono",
    "cube",
    "semantic-layer"
  ],
  "author": "Your Name",
  "license": "MIT"
}
